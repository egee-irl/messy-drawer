{
  "name": "ts-skeleton",
  "version": "1.0.0",
  "description": "a typescript template for node projects",
  "main": "index.js",
  "repository": "git@github.com:egee-irl/ts-skeleton.git",
  "author": "egee-irl <brian@egee.io>",
  "license": "MIT",
  "scripts": {
    "start": "node dist/index.js",
    "build": "node_modules/.bin/tsc --build tsconfig.json",
    "rebuild": "yarn clean && yarn run build",
    "clean": "rm -rf dist/* && touch dist/.gitkeep",
    "test": "node_modules/.bin/jest --runInBand --ci --reporters=default --reporters=jest-junit"
  },
  "dependencies": {
    "@types/node": "^12.7.4",
    "typescript": "^3.6.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "jest": "^24.9.0",
    "jest-junit": "^8.0.0",
    "ts-jest": "^24.0.2"
  }
}
